00196 {\cf20 //                           CÃLCULO DEL ÃNDICE PMV}\par
00197 {\cf20 // ============================================================================}\par
00198 {\cf18 float} calcularPMV({\cf18 float} temperatura, {\cf18 float} humedad) \{\par
00199   {\cf20 // Modelo simplificado del Ã­ndice PMV}\par
00200   {\cf17 const} {\cf18 float} Tn = 22.0f; {\cf20 // temperatura neutra}\par
00201   {\cf17 const} {\cf18 float} Hn = 50.0f; {\cf20 // humedad neutra}\par
00202   {\cf18 float} pmv = 0.3f * (temperatura - Tn) + 0.01f * (humedad - Hn);\par
00203   {\cf19 return} pmv;\par
00204 \}\par
