00297 {\cf20 // ============================================================================}\par
00298 {\cf20 //                      FUNCI\'C3\'83\'C2\'93N PARA MOSTRAR EL PMV EN PANTALLA}\par
00299 {\cf20 // ============================================================================}\par
00300 {\cf18 void} mostrarPMV({\cf18 float} pmv) \{\par
00301   lcd.setCursor(0, 0);\par
00302   lcd.print({\cf22 "Temp: "});\par
00303   lcd.print(temperaturaMedia, 1);\par
00304   lcd.print({\cf22 "C"});\par
00305   lcd.setCursor(0, 1);\par
00306   lcd.print({\cf22 "PMV: "});\par
00307   lcd.print(pmv, 2);\par
00308   lcd.print({\cf22 "  "});\par
00309 \}\par
