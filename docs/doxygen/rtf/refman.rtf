00195 {\cf20 // ============================================================================}\par
00196 {\cf20 //                           C\'C3\'83\'C2\'81LCULO DEL \'C3\'83\'C2\'8DNDICE PMV}\par
00197 {\cf20 // ============================================================================}\par
00198 {\cf18 float} calcularPMV({\cf18 float} temperatura, {\cf18 float} humedad) \{\par
00199   {\cf20 // Modelo simplificado del \'C3\'ADndice PMV}\par
00200   {\cf17 const} {\cf18 float} Tn = 22.0f; {\cf20 // temperatura neutra}\par
00201   {\cf17 const} {\cf18 float} Hn = 50.0f; {\cf20 // humedad neutra}\par
00202   {\cf18 float} pmv = 0.3f * (temperatura - Tn) + 0.01f * (humedad - Hn);\par
00203   {\cf19 return} pmv;\par
00204 \}\par
00205 \par
00206 \par
00207 {\cf20 // ============================================================================}\par
00208 {\cf20 //                     CONFIGURACI\'C3\'83\'C2\'93N DE M\'C3\'83\'C2\'81QUINA DE ESTADOS}\par
00209 {\cf20 // ============================================================================}\par
00210 {\cf18 void} setupStateMachine() \{\par
00211   State* Monitor   = stateMachine.AddState({\cf22 "Monitor"}, []() \{\par
00212     mostrarMensaje({\cf22 "Monitoreando..."});\par
00213   \});\par
00214   State* pmv_alto  = stateMachine.AddState({\cf22 "pmv_alto"}, []() \{\par
00215     mostrarMensaje({\cf22 "Activando ventilador"});\par
00216     digitalWrite(pinVentilador, HIGH);\par
00217     digitalWrite(pinCalefactor, LOW);\par
00218   \});\par
00219   State* pmv_bajo  = stateMachine.AddState({\cf22 "pmv_bajo"}, []() \{\par
00220     mostrarMensaje({\cf22 "Activando calefactor"});\par
00221     digitalWrite(pinVentilador, LOW);\par
00222     digitalWrite(pinCalefactor, HIGH);\par
00223   \});\par
00224 \par
00225   {\cf20 // --- Transiciones --- //}\par
00226   stateMachine.AddTransition(Monitor, pmv_alto, []() \{\par
00227     {\cf19 return} input == pmv && pmv_actual > 1.0f;\par
00228   \});\par
00229   stateMachine.AddTransition(Monitor, pmv_bajo, []() \{\par
00230     {\cf19 return} input == pmv && pmv_actual < -1.0f;\par
00231   \});\par
00232   stateMachine.AddTransition(pmv_alto, Monitor, []() \{\par
00233     {\cf19 return} pmv_actual <= 1.0f;\par
00234   \});\par
00235   stateMachine.AddTransition(pmv_bajo, Monitor, []() \{\par
00236     {\cf19 return} input == tiempo || pmv_actual >= -1.0f;\par
00237   \});\par
00238 \}\par
00239 \par
00240 \par
00241 {\cf20 // ============================================================================}\par
00242 {\cf20 //                     FUNCI\'C3\'83\'C2\'93N DE CONTROL AUTOM\'C3\'83\'C2\'81TICO DEL CLIMA}\par
00243 {\cf20 // ============================================================================}\par
00244 {\cf18 void} controlarClima({\cf18 float} pmv) \{\par
00245   {\cf19 if} (sistemaPausado) {\cf19 return}; {\cf20 // No actuar si est\'C3\'A1 pausado}\par
00246 \par
00247   {\cf19 if} (pmv > 1.0f) \{ {\cf20 // Demasiado calor}\par
00248     digitalWrite(pinVentilador, HIGH);\par
00249     digitalWrite(pinCalefactor, LOW);\par
00250     mostrarMensaje({\cf22 "Ventilador ON"});\par
00251   \} \par
00252   {\cf19 else} {\cf19 if} (pmv < -1.0f) \{ {\cf20 // Demasiado fr\'C3\'ADo}\par
00253     digitalWrite(pinVentilador, LOW);\par
00254     digitalWrite(pinCalefactor, HIGH);\par
00255     mostrarMensaje({\cf22 "Calefactor ON"});\par
00256   \} \par
00257   {\cf19 else} \{ {\cf20 // Confort t\'C3\'A9rmico}\par
00258     digitalWrite(pinVentilador, LOW);\par
00259     digitalWrite(pinCalefactor, LOW);\par
00260     mostrarMensaje({\cf22 "Confort t\'C3\'A9rmico"});\par
00261   \}\par
00262 \}\par
00263 \par
00264 \par
00265 {\cf20 // ============================================================================}\par
00266 {\cf20 //                     FUNCI\'C3\'83\'C2\'93N DE CONTROL MANUAL (por teclado)}\par
00267 {\cf20 // ============================================================================}\par
00268 {\cf18 void} controlManual() \{\par
00269   {\cf18 char} tecla = teclado.getKey();\par
00270   {\cf19 if} (tecla) \{\par
00271     {\cf19 if} (tecla == {\cf23 'A'}) \{\par
00272       digitalWrite(pinVentilador, HIGH);\par
00273       mostrarMensaje({\cf22 "Ventilador manual ON"});\par
00274     \} {\cf19 else} {\cf19 if} (tecla == {\cf23 'B'}) \{\par
00275       digitalWrite(pinCalefactor, HIGH);\par
00276       mostrarMensaje({\cf22 "Calefactor manual ON"});\par
00277     \} {\cf19 else} {\cf19 if} (tecla == {\cf23 'C'}) \{\par
00278       digitalWrite(pinVentilador, LOW);\par
00279       digitalWrite(pinCalefactor, LOW);\par
00280       mostrarMensaje({\cf22 "Apagado manual"});\par
00281     \}\par
00282   \}\par
00283 \}\par
00284 \par
