00104 {\cf20 //                                LOOP PRINCIPAL}\par
00105 {\cf20 // ============================================================================}\par
00106 {\cf18 void} loop() \{\par
00107   botonBloqueo.loop();\par
00108 \par
00109   {\cf20 // Si se presiona el bot\'C3\'B3n de bloqueo:}\par
00110   {\cf19 if} (botonBloqueo.isPressed()) \{\par
00111     sistemaBloqueado = !sistemaBloqueado; {\cf20 // Cambiar estado del sistema}\par
00112 \par
00113     {\cf19 if} (sistemaBloqueado) \{\par
00114       mostrarMensaje({\cf22 "Sistema BLOQUEADO"});\par
00115       digitalWrite(pinVentilador, LOW);\par
00116       digitalWrite(pinCalefactor, LOW);\par
00117     \} {\cf19 else} \{\par
00118       mostrarMensaje({\cf22 "Sistema DESBLOQUEADO"});\par
00119     \}\par
00120   \}\par
00121 \par
00122   {\cf20 // Si est\'C3\'A1 bloqueado, no hace nada m\'C3\'A1s}\par
00123   {\cf19 if} (sistemaBloqueado) {\cf19 return};\par
00124 \par
00125   {\cf20 // Leer entrada de sensores o teclado}\par
00126   input = leerEntrada();\par
00127   stateMachine.Update();\par
00128 \}\par
00129 \par
00130 \par
00131 {\cf20 // ============================================================================}\par
00132 {\cf20 //                            FUNCI\'C3\'83\'C2\'93N LEER ENTRADA}\par
00133 {\cf20 // ============================================================================}\par
00134 Input leerEntrada() \{\par
00135   {\cf20 // Leer temperaturas y humedad}\par
00136   temperaturaDHT = dht.readTemperature();\par
00137   humedadDHT = dht.readHumidity();\par
00138   {\cf18 int} valorNTC = analogRead(pinNTC);\par
00139   temperaturaNTC = map(valorNTC, 0, 1023, -20, 50); {\cf20 // Aprox. en \'C2\'B0C}\par
00140   temperaturaMedia = (temperaturaDHT + temperaturaNTC) / 2;\par
00141 \par
00142   pmv_actual = calcularPMV(temperaturaMedia, humedadDHT);\par
00143 \par
00144   {\cf20 // --- Estados de m\'C3\'A1quina de estados seg\'C3\'BAn variable actual --- //}\par
00145   State* estadoActual = stateMachine.GetState();\par
00146 \par
00147   {\cf20 // Estado: pmv_alto \'C3\'A2\'C2\'86\'C2\'92 volver a monitoreo si vuelve al rango}\par
00148   {\cf19 if} (estadoActual->GetName() == {\cf22 "pmv_alto"} && pmv_actual <= 1.0f) \{\par
00149     mostrarMensaje({\cf22 "Regreso al confort"});\par
00150     digitalWrite(pinVentilador, LOW);\par
00151     delay(1000);\par
00152     {\cf19 return} Input::pmv;\par
00153   \}\par
00154 \par
00155   {\cf20 // Estado: Monitor \'C3\'A2\'C2\'86\'C2\'92 evaluar cambios}\par
00156   {\cf19 if} (estadoActual->GetName() == {\cf22 "Monitor"}) \{\par
00157     {\cf19 if} (pmv_actual > 1.0f) \{\par
00158       mostrarMensaje({\cf22 "Temperatura alta"});\par
00159       {\cf19 return} Input::pmv;\par
00160     \}\par
00161     {\cf19 if} (pmv_actual < -1.0f) \{\par
00162       mostrarMensaje({\cf22 "Temperatura baja"});\par
00163       {\cf19 return} Input::pmv;\par
00164     \}\par
00165   \}\par
00166 \par
00167   {\cf20 // Lectura de teclado para control manual}\par
00168   {\cf18 char} tecla = teclado.getKey();\par
00169   {\cf19 if} (tecla) \{\par
00170     {\cf19 if} (tecla == {\cf23 '*'}) \{\par
00171       sistemaPausado = !sistemaPausado;\par
00172       mostrarMensaje(sistemaPausado ? {\cf22 "Sistema pausado"} : {\cf22 "Sistema activo"});\par
00173     \} {\cf19 else} {\cf19 if} (tecla == {\cf23 '#'}) \{\par
00174       temperaturaIdeal = temperaturaMedia;\par
00175       mostrarMensaje({\cf22 "Nueva T ideal: "} + String(temperaturaIdeal) + {\cf22 "C"});\par
00176     \}\par
00177   \}\par
00178 \par
00179   {\cf20 // Si hay DHT disponible}\par
00180   {\cf19 if} (!isnan(temperaturaDHT) && !isnan(humedadDHT)) \{\par
00181     mostrarPMV(pmv_actual);\par
00182     controlarClima(pmv_actual);\par
00183     {\cf19 return} Input::dht;\par
00184   \}\par
00185 \par
00186   {\cf20 // Si no hay DHT, pero s\'C3\'AD NTC}\par
00187   {\cf19 if} (!isnan(temperaturaNTC)) \{\par
00188     {\cf19 return} Input::ntc;\par
00189   \}\par
00190 \par
00191   {\cf19 return} Input::desconocido;\par
00192 \}\par
00193 \par
