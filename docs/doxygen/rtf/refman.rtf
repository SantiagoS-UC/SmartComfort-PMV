00286 {\cf20 // ============================================================================}\par
00287 {\cf20 //                        FUNCI\'C3\'83\'C2\'93N PARA MOSTRAR MENSAJES}\par
00288 {\cf20 // ============================================================================}\par
00289 {\cf18 void} mostrarMensaje(String mensaje) \{\par
00290   lcd.clear();\par
00291   lcd.setCursor(0, 0);\par
00292   lcd.print(mensaje);\par
00293   Serial.println(mensaje);\par
00294 \}\par
